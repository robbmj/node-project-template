{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "./src"
        ],
        "includePattern": ".+\\.js"
    },
    "plugins": [
        "node_modules/jsdoc-strip-async-await/index",
        "plugins/markdown"

    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "includeDate": true,
            "outputSourceFiles": false
        }
    }
}
